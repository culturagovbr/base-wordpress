     forceCoversAnnotation="false"
     mapTestClassNameToCoveredClassName="false"
     bootstrap="../MovimentarAmbiente.class.php"
     processIsolation="false"
     stopOnError="false"
     stopOnFailure="false"
     stopOnIncomplete="false"
     stopOnSkipped="false"

     strict="false"
     verbose="false"
     >
  
  <php>
    <var name="DB_DSN" value="mysql:dbname=wpminc_unittest;host=localhost" />
    <var name="DB_USER" value="root" />
    <var name="DB_PASSWD" value="" />
    <var name="DB_DBNAME" value="wpminc_unittest" />
  </php>
  <testsuites>
    <testsuite name="AllTests">
      <directory>.</directory>
    </testsuite>
  </testsuites>
  
</phpunit>
